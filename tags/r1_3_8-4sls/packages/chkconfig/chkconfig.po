msgid ""
"This may be freely redistributed under the terms of the GNU Public License.\n"
msgstr ""
"此軟件可根據 GNU Public License 的條款自由發佈。\n"

#: ../chkconfig.c:26
#, c-format
msgid "usage:   %s --list [name]\n"
msgstr ""

#: ../chkconfig.c:27
#, c-format
msgid "         %s --add <name>\n"
msgstr ""

#: ../chkconfig.c:28
#, c-format
msgid "         %s --del <name>\n"
msgstr ""

#: ../chkconfig.c:29
#, c-format
msgid "         %s [--level <levels>] <name> %s)\n"
msgstr ""

#: ../chkconfig.c:36
#, c-format
msgid "service %s does not support chkconfig\n"
msgstr "%s 服務不支援 chkconfig\n"

#: ../chkconfig.c:38
#, c-format
msgid "error reading information on service %s: %s\n"
msgstr "讀取 %s 服務的資料發生錯誤 : %s\n"

#: ../chkconfig.c:102 ../chkconfig.c:107 ../chkconfig.c:204
msgid "on"
msgstr "開啟"

#: ../chkconfig.c:102 ../chkconfig.c:107 ../chkconfig.c:204
msgid "off"
msgstr "關閉"

#: ../chkconfig.c:139
#, c-format
msgid "failed to open %s/init.d: %s\n"
msgstr "未能開啟 %s/init.d: %s\n"

#: ../chkconfig.c:173
#, c-format
msgid "error reading from directory %s/init.d: %s"
msgstr "讀取 %s/init.d 發生錯誤 : %s"

#: ../chkconfig.c:181
msgid "xinetd based services:\n"
msgstr "建於 xinetd 上的服務:\n"

#: ../chkconfig.c:183
#, c-format
msgid "failed to open directory %s: %s"
msgstr "未能開啟 %s: %s"

#: ../chkconfig.c:285
#, c-format
msgid "%s version %s\n"
msgstr "%s 版本 %s\n"

#: ../chkconfig.c:292
msgid "only one of --list, --add, or --del may be specified\n"
msgstr "只能選擇 --list, --add 或 --del 其中之一\n"

#: ../chkconfig.c:340
msgid "only one runlevel may be specified for a chkconfig query\n"
msgstr "每次只可詢問一種 runlevel\n"

#: ../ntsysv.c:30
msgid "Press <F1> for more information on a service."
msgstr "如想知道本服務的詳情可按 <F1>."

#: ../ntsysv.c:62 ../ntsysv.c:99
msgid "Ok"
msgstr "確定"

#: ../ntsysv.c:62
msgid "Back"
msgstr "返回"

#: ../ntsysv.c:62
msgid "Cancel"
msgstr "取消"

#: ../ntsysv.c:74
msgid "What services should be automatically started?"
msgstr "什麼服務將被自動啟動?"

#: ../ntsysv.c:80
msgid "Services"
msgstr "服務"

#: ../ntsysv.c:174 ../ntsysv.c:217
#, c-format
msgid "error reading info for service %s: %s\n"
msgstr "讀取 %s 服務有關資料時發生錯誤 : %s \n"

#: ../ntsysv.c:183
#, c-format
msgid "error reading from directory %s/init.d: %s\n"
msgstr "從 %s/init.d 讀取時發生錯誤 : %s \n"

#: ../ntsysv.c:226
#, c-format
msgid "error reading from directory %s: %s\n"
msgstr "從 %s 讀取時發生錯誤 : %s \n"

#: ../ntsysv.c:270
msgid "bad argument to --levels\n"
msgstr "--level 的引數並不正確\n"

#: ../ntsysv.c:277
msgid "No services may be managed by ntsysv!\n"
msgstr "ntsysv 無法管理任何服務!\n"

#: ../leveldb.c:355
#, c-format
msgid "failed to glob pattern %s: %s\n"
msgstr "找 %s 樣式失敗: %s\n"

#: ../leveldb.c:386
msgid "cannot determine current run level\n"
msgstr "未能找出目前的 run level\n"

#: ../leveldb.c:474
#, c-format
msgid "failed to make symlink %s: %s\n"
msgstr "不能建立 symlink %s: %s\n"

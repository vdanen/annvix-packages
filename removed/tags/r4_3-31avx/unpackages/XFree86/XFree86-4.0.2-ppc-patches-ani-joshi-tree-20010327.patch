+
+    pMga->AccelFlags |= CLIPPER_ON;
+    pMga->expandRows = h;
+    pMga->expandDWORDs = ((w * PSZ) + 31) >> 5;
+
+    WAITFIFO(5);
+    OUTREG(MGAREG_CXBNDRY, 0xFFFF0000 | ((x + skipleft) & 0xFFFF));
+    OUTREG(MGAREG_AR0, w - 1);
+    OUTREG(MGAREG_AR3, 0);
+    OUTREG(MGAREG_FXBNDRY, ((x + w - 1) << 16) | (x & 0xFFFF));

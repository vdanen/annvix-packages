+    OUTREG(MGAREG_YTOP, 0x00000000);        /* minPixelPointer */ 
+    OUTREG(MGAREG_YBOT, 0x007FFFFF);        /* maxPixelPointer */ 
+    pMga->AccelFlags &= ~CLIPPER_ON;
+}

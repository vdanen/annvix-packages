+Bool 
+MGAStormAccelInit(ScreenPtr pScreen)
+{ 
+    ScrnInfoPtr pScrn = xf86Screens[pScreen->myNum];
+
+    switch( pScrn->bitsPerPixel )
+    {
+    case 8:
+    	return Mga8AccelInit(pScreen);
+    case 16:
+    	return Mga16AccelInit(pScreen);
+    case 24:
+    	return Mga24AccelInit(pScreen);
+    case 32:

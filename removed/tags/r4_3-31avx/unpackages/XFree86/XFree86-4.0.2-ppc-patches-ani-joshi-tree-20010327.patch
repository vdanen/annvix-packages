+	esac; \
+	echo "making Makefiles in $$curdir$$i..."; \
+	itmp=`echo $$i | sed -e 's;^\./;;g' -e 's;/\./;/;g'`; \
+	curtmp="$(CURRENT_DIR)" \
+	toptmp=""; \
+	case "$$itmp" in \
+	../?*) \
+	while echo "$$itmp" | grep '^\.\./' > /dev/null;\
+	do \
+	toptmp="/`basename $$curtmp`$$toptmp"; \
+	curtmp="`dirname $$curtmp`"; \
+	itmp="`echo $$itmp | sed 's;\.\./;;'`"; \
+	done \
+	;; \
+	esac; \
+	case "$$itmp" in \
+	*/?*/?*/?*/?*)	newtop=../../../../..;; \
+	*/?*/?*/?*)	newtop=../../../..;; \
+	*/?*/?*)	newtop=../../..;; \
+	*/?*)		newtop=../..;; \
+	*)		newtop=..;; \
+	esac; \
+	newtop="$$newtop$$toptmp"; \
+	case "$(TOP)" in \
+	/?*) imaketop=$(TOP) \
+	imakeprefix= ;; \
+	.) imaketop=$$newtop \
+	imakeprefix=$$newtop/ ;; \
+	*) imaketop=$$newtop/$(TOP) \
+	imakeprefix=$$newtop/ ;; \
+	esac; \
+	$(RM) $$i/Makefile.bak; \
+	if [ -f $$i/Makefile ]; then \
+	echo "	$(MV) Makefile Makefile.bak"; \
+	if [ "$$executeit" != "no" ]; then \
+	$(MV) $$i/Makefile $$i/Makefile.bak; \
+	fi; \
+	fi; \
+	$(MAKE) $(MFLAGS) $(MAKE_OPTS) ONESUBDIR=$$i ONECURDIR=$$curdir IMAKETOP=$$imaketop IMAKEPREFIX=$$imakeprefix $$i/Makefile; \
+	if [ -d $$i ] ; then \
+	cd $$i; \
+	$(MAKE) $(MFLAGS) Makefiles; \
+	cd $$newtop; \
+	else \
+	exit 1; \
+	fi; \
+	done
+
+includes::
+	@for flag in ${MAKEFLAGS} ''; do \
+	case "$$flag" in *=*) ;; *[ik]*) set +e;; esac; done; \
+	for i in $(SUBDIRS) ;\
+	do \
+	echo including "in $(CURRENT_DIR)/$$i..."; \
+	$(MAKE) -C $$i $(MFLAGS) $(PARALLELMFLAGS)  includes; \
+	done

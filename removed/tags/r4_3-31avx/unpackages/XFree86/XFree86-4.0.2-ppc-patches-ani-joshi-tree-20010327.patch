+    }
+
+    maxlines = (min(pMga->FbUsableSize, 16*1024*1024)) / 
+	               (pScrn->displayWidth * PSZ / 8);
+
+#ifdef XF86DRI
+    if (pMga->directRenderingEnabled) {
+	if(maxlines > (pMga->numXAALines + pScrn->virtualY)) 
+	     maxlines = pMga->numXAALines + pScrn->virtualY;

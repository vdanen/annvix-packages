+all:: $(DRIVER)_drv.o
+
+$(DRIVER)_drv.o:  $(OBJS) $(EXTRALIBRARYDEPS)
+	$(RM) $@
+	$(LD) $(LDCOMBINEFLAGS)  $(OBJS) -o $@
+
+all:: $(DRIVER)_drv.o
+	@if [ -d $(BUILDMODULEDIR)/input ]; then set +x; \
+	else (set -x; $(MKDIRHIER) $(BUILDMODULEDIR)/input); fi
+	$(RM) $(BUILDMODULEDIR)/input/$(DRIVER)_drv.o
+	@(UP="`$(REVPATH) input`"; set -x; \
+	cd $(BUILDMODULEDIR)/input && \
+	$(LN) $(BUILDMODULETOP)/$${UP}$(CURRENT_DIR)/$(DRIVER)_drv.o .)
+
+install:: $(DRIVER)_drv.o
+	@if [ -d $(DESTDIR)$(MODULEDIR)/input ]; then set +x; \
+	else (set -x; $(MKDIRHIER) $(DESTDIR)$(MODULEDIR)/input); fi
+	$(INSTALL) -c $(DRIVER)_drv.o $(DESTDIR)$(MODULEDIR)/input
+
+all:: $(DRIVER).man
+
+$(DRIVER).man::  $(DRIVER).cpp
+	$(RM) $@
+	 $(RAWCPP)  $(MANDEFS)  <$(DRIVER).cpp | sed -e '/^#  *[0-9][0-9]*  *.*$$/d' -e '/^XCOMM$$/s//#/' -e '/^XCOMM[^a-zA-Z0-9_]/s/^XCOMM/#/' >$@
+
+clean::
+	$(RM) $(DRIVER).man
+
+all:: $(DRIVER).$(DRIVERMANSUFFIX).html
+
+$(DRIVER).$(DRIVERMANSUFFIX).html: $(DRIVER).man $(RMAN)
+	$(RM) $(DRIVER).$(DRIVERMANSUFFIX).html $(DRIVER).$(DRIVERMANSUFFIX)-html
+	$(RMAN) $(RMANOPTIONS) < $(DRIVER).man > $(DRIVER).$(DRIVERMANSUFFIX)-html && \
+	$(MV) $(DRIVER).$(DRIVERMANSUFFIX)-html $@
+
+install.man:: $(DRIVER).$(DRIVERMANSUFFIX).html
+	@if [ -d $(DESTDIR)$(DOCHTMLDIR) ]; then set +x; \
+	else (set -x; $(MKDIRHIER) $(DESTDIR)$(DOCHTMLDIR)); fi
+	@(SUF=`expr $(DRIVERMANSUFFIX) \: '\(.\)'`; \
+	set -x; \
+	$(INSTALL) $(INSTALLFLAGS) $(INSTMANFLAGS) $(DRIVER).$(DRIVERMANSUFFIX).html $(DESTDIR)$(DOCHTMLDIR)/$(DRIVER).$$SUF.html)
+

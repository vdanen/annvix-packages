+	else
+#endif
+            mgaCMD |= MGADWG_TRANSC | pMga->Atype[rop];
+
+	WAITFIFO(4);
+    } else {
+#if PSZ == 24
+	if((pMga->AccelFlags & BLK_OPAQUE_EXPANSION) && 
+		RGBEQUAL(fg) && RGBEQUAL(bg)) 
+#else
+	if((pMga->AccelFlags & BLK_OPAQUE_EXPANSION)) 
+#endif
+        	mgaCMD |= pMga->Atype[rop];
+	else
+        	mgaCMD |= pMga->AtypeNoBLK[rop];
+	WAITFIFO(5);
+    	SET_BACKGROUND(bg);
+    }
+
+    SET_FOREGROUND(fg);

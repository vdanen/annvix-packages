+
+    name[Group1]= "Cyrillic";
+
+    key <AE01> { [ exclam,       1          	]	};
+    key <AE02> { [ minus,        2          	]	};
+    key <AE03> { [ quotedbl,     3          	]	};
+    key <AE04> { [ Cyrillic_ie,  4          	]	};
